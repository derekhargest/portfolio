setTimeout(function(){
    $('#odometer1').html('12480');
    $('#odometer2').html('1010');
  $('#odometer3').html('2');
  $('#odometer4').html('1000000');
    }, 1000);
                            
                            });



$(document)
	.on('click', 'a[href^="#"]', function (e) {
    e.preventDefault();

    $('html, body').animate({
        scrollTop: $($.attr(this, 'href')).offset().top
    }, 500);
});




  
